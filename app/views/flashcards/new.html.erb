<% provide(:title, 'New Flashcard') %>

<div class="col-md-6 col-md-offset-1">
<h1>Create a new Flashcard</h1>
</div>

<div class="row">
  <div class="col-md-6 col-md-offset-1">
    <%= form_for(@flashcard) do |f| %>
        <%= f.label :question %>
        <%= f.text_field :question %>

        <%= f.label :answer %>
        <%= f.text_field :answer %>
        
        <%= f.submit %>

    <% end %>
  </div>
</div class="col-md-6 col-md-offset-1">
</div>



